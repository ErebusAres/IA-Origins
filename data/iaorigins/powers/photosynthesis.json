{
    "type": "origins:multiple",
    "regenerate_food": {
      "type": "origins:action_over_time",
      "condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:daytime"
          },
          {
            "type": "origins:exposed_to_sky"
          }
        ]
      },
      "interval": 20,
      "entity_action": {
        "type": "origins:feed",
        "food": 1,
        "saturation": 1
      }
    },
    "prevent_healing": {
      "type": "origins:disable_regen"
    },
    "heal_override": {
      "type": "origins:action_over_time",
      "condition": {
        "type": "origins:food_level",
        "comparison": ">",
        "compare_to": 0
      },
      "interval": 30,
      "entity_action": {
        "type": "origins:heal",
        "amount": 1
      }
    },
    "exhaust_when_healing": {
      "type": "origins:action_over_time",
      "condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:food_level",
            "comparison": ">",
            "compare_to": 0
          },
          {
            "type": "origins:relative_health",
            "comparison": "<",
            "compare_to": 1
          }
        ]
      },
      "interval": 30,
      "entity_action": {
        "type": "origins:exhaust",
        "amount": 2
      }
    },
    "name": "Photosynthesis",
    "description": "You recover hunger in daylight."
  }